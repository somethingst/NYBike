<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>nybike</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>TripDayCountServlet</servlet-name>
    <servlet-class>cn.tedu.nybike.web.TripDayCountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TripDayCountServlet</servlet-name>
    <url-pattern>/tripDayCount</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DateInfoServlet</servlet-name>
    <servlet-class>cn.tedu.nybike.web.DateInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DateInfoServlet</servlet-name>
    <url-pattern>/dateInfo</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DateInfo2Servlet</servlet-name>
    <servlet-class>cn.tedu.nybike.web.DateInfo2Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DateInfo2Servlet</servlet-name>
    <url-pattern>/dateInfo2</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>TripDayHourCountServlet</display-name>
    <servlet-name>TripDayHourCountServlet</servlet-name>
    <servlet-class>cn.tedu.nybike.web.TripDayHourCountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TripDayHourCountServlet</servlet-name>
    <url-pattern>/tripDayHourCount</url-pattern>
  </servlet-mapping>
</web-app>